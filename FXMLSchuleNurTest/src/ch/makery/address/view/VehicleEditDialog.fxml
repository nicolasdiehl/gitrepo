                        <TextField fx:id="geliehenField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
                  <Button layoutX="256.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleOk" text="OK" />
                  <Label layoutX="275.0" layoutY="23.0" text="Vehicle Editieren">
                     <font>
                        <Font name="System Bold Italic" size="13.0" />
                     </font></Label>
                  <Button layoutX="318.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleCancel" text="Abbrechen" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
   <effect>
      <Lighting diffuseConstant="2.0">
         <bumpInput>
            <Shadow />
