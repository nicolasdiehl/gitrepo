<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.VehicleEditDialogController">
   <children>
      <AnchorPane>
         <children>
            <AnchorPane prefHeight="400.0" prefWidth="600.0">
               <children>
                  <GridPane layoutX="73.0" layoutY="91.0" prefHeight="234.0" prefWidth="454.0">
